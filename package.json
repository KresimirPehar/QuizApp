        "react-fontawesome": "^1.6.1",
        "react-redux": "^6.0.0",
        "react-router-dom": "^4.3.1",
        "react-scripts": "2.1.1",
        "reactparticles.js": "^1.1.6",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "axios": "^0.18.0",
        "body-parser": "^1.18.3",
        "cors": "^2.8.5",
        "express": "^4.16.4",
        "express-session": "^1.15.6",
